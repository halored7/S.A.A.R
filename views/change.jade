div(class="modal fade" id="modalChange" tabindex="-1" role="dialog" aria-hidden="true")
	div(class="modal-dialog")
		div(class="modal-content")
			div(class="modal-header")
				button(type="button" class="close" data-dismiss="modal")
					span(aria-hidden="true") &times;
					span(class="sr-only") Cerrar
				h2(class="modal-title") Cambio de datos
			div(class="modal-body")
				form(class="form-horizontal" id="form_changeData")
					div(class="form-group")
						label(for="name" class="control-label col-xs-5") Nombre de usuario:
						div(class="col-xs-5")
							input(type="text" id="name" name="name" class="form-control")
					div(class="form-group")
						label(for="password" class="control-label col-xs-5") Contrase√±a:
						div(class="col-xs-5")
							input(type="text" id="password" name="password" class="form-control")
			div(class="modal-footer")
				button(type="button" onclick="change();" id="changeData" class="btn btn-primary") Cambiar datos
				button(type="button" class="btn btn-danger" data-dismiss="modal") Cancelar